<script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
<link href="//netdna.bootstrapcdn.com/bootstrap/3.0.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">

<script src="https://code.jquery.com/jquery-2.2.4.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
<link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet"/>
<script src="https://cdnjs.cloudflare.com/ajax/libs/1000hz-bootstrap-validator/0.11.9/validator.min.js"></script>

<!-- Datepicker -->
<link href='bootstrap-datepicker/dist/css/bootstrap-datepicker.min.css' rel='stylesheet' type='text/css'>
<script src='bootstrap-datepicker/dist/js/bootstrap-datepicker.min.js' type='text/javascript'></script>
<!------ Include the above in your HEAD tag ---------->


<style type="text/css">
    body { background: gray !important; } /* Adding !important forces the browser to overwrite the default style applied by Bootstrap */
  </style>
<ul class="list-group shadow">
  <% for (const event of model) { %>
<div class="container" id="wrap">
	  <div class="row">
        <div class="col-md-6 col-md-offset-3">
            <form action="/volunteer-event" method="post" accept-charset="utf-8" class="form" data-toggle="validator" role="form" >   <legend>Event</legend>
              <input type="hidden" id="eventId" name="eventId" value="<%= event.Id %>">  
              <div class="form-group">
                    <input type="text" name="event" disabled value=<%= event.Name %> class="form-control input-lg" placeholder="Event" maxlength="50"  required /> 
                    <div class="help-block with-errors"></div>
                </div>

                <div class="form-group">
                    <input type="text" name="description" disabled value="<%= event.Description %>" class="form-control input-lg" placeholder="Description" maxlength="500"  p required /> 
                    <div class="help-block with-errors"></div>
                </div>
                <input type="text" name="totalpositions" disabled value="<%= event.TotalPositions %>" class="form-control input-lg" placeholder="Total No Of Positions" maxlength="3"  pattern="[0-9]*" required />
                
                <div class='form-group'>
 
                    <input type='text' class="form-control input-lg" disabled id='datepicker' value="<%= event.Date %>" name="date" placeholder='Date' style='width: 300px;'  >
                   
                   </div>
                
                
                <div class="row">
                    <div class="col-xs-6 col-md-6">
                        <input type="text" name="begintime" disabled class="form-control input-lg" value="<%= event.BeginTime %>" placeholder="Begin Time"  />                        </div>
                    <div class="col-xs-6 col-md-6">
                        <input type="text" name="endtime" disabled class="form-control input-lg" value="<%= event.EndTime %>" placeholder="End Time"  />                        </div>
                </div>
                <div class="row">
                        <div class="col-xs-6 col-md-6">
                            <input type="text" name="contactfirstname" disabled class="form-control input-lg" placeholder="Contact First Name" value=<%= event.ContactFirstName %> maxlength="30"  pattern="[A-Za-z]*" required  />                        </div>
                        <div class="col-xs-6 col-md-6">
                            <input type="text" name="contactlastname" disabled value="<%= event.ContactLastName %>" class="form-control input-lg" placeholder="Contact Last Name" maxlength="30"  pattern="[A-Za-z]*" required  />                        </div>
                    </div>
                    
                    
                    <div class="form-group">
                        <input type="email" disabled name="contactemail" value="<%= event.ContactEmailAddress %>" class="form-control input-lg" maxlength="100" placeholder="Contact Email"  />
                        <div class="help-block with-errors"></div>
                    </div>
                                    
                    <input type="address1" name="address1" disabled value="<%= event.Address1 %>" maxlength="100" class="form-control input-lg" placeholder="Address" required />
                    <input type="address2" name="address2" disabled value="<%= event.Address2 %>" maxlength="100" class="form-control input-lg" placeholder="Apartment, studio, or floor"  /> 
 
                     <div class="row">
                        <div class="col-xs-6 col-md-6">
                          <input type="text" name="city" disabled value="<%= event.City %>" maxlength="100" class="form-control input-lg" placeholder="City" required /></div>
                        <div class="col-xs-6 col-md-6">
                          <input type="text" name="state"  disabled value="<%= event.State %>" maxlength="100" class="form-control input-lg" placeholder="State" required />    </div>                
                        <div class="col-xs-6 col-md-6">
                          <input type="text" name="zip" disabled value="<%= event.Zip %>" maxlength="100" class="form-control input-lg" placeholder="Zip" required />    </div>                       
         
    
                    </div>
  
                   
                    <br />
                    <button class="btn btn-lg btn-primary btn-block signup-btn" type="submit">
                        Subscribe</button>
                        <div class="col">
                            <!-- Simple link -->
                            <a href="/subscribed-events">Back to Subscribed Events</a>
                          </div>
            </form>          
          </div>
</div>            
</div>
<% } %>
        </ul>
</div>

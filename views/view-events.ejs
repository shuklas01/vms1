<style type="text/css">
    body { background: gray !important; } /* Adding !important forces the browser to overwrite the default style applied by Bootstrap */
 </style>
 

<div class="container py-5">

    <!-- For demo purpose -->
    <div class="row text-left text-navy mb-5">
      <div class="col-lg-7 mx-auto">
        <h1 class="display-4">Events</h1>
    

      </div>
    </div>
    <!-- End -->
  
    <div class="row">
      <div class="col-lg-8 mx-auto">
  
        <!-- List group-->
        <ul class="list-group shadow">
            <% for (const event of model) { %>
          <!-- list group item-->
          <li class="list-group-item">
            <!-- Custom content-->
            <div class="media align-items-lg-center flex-column flex-lg-row p-3">
              <div class="media-body order-2 order-lg-1">
                <a href="/test/"><%= event.Name %></a>
                <p class="font-italic text-muted mb-0 small"><%= event.Description %></p>
                <div class="d-flex align-items-center justify-content-between mt-1">
                  <h6 class="font-weight-bold my-2">Contact Name : <%= event.ContactFirstName %> <%= event.ContactLastName %></h6>
                  
                  
                </div>
                <h7 class="font-weight-bold my-2">Contact Email : <%= event.ContactEmailAddress %></h7>
                <h8 class="font-weight-bold my-2">Location : <%= event.Address1 %>, <%= event.City %>, <%= event.State %>, <%= event.Zip %></h8>
                
            </div>
            <!-- End -->
          </li>
          <!-- End -->
  
          <% } %>
        </ul>
        <!-- End -->
      </div>
    </div>
    <div class="col">
        <!-- Simple link -->
        <a href="/add-event">Add New Event</a>
      </div>
  </div>